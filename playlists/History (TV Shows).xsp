<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>History (TV Shows)</name>
    <match>10</match>
    <rule field="lastplayed" operator="inthelast">
        <value>7 days</value>
    </rule>
    <order direction="ascending">lastplayed</order>
</smartplaylist>
